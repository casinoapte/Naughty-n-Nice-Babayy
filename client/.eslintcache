ebVitals.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/App.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/pages/SignInPage/SignIn.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/pages/HomePage/HomePage.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/pages/main-group-page/MainGroupPage.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/Sidebar/Sidebar.js",["81","82"],"import React from 'react';\nimport styled from 'styled-components';\nimport { BrowserRouter as Router, Route, Link, withRouter } from \"react-router-dom\";\n\nconst StyledSideNav = styled.div` \n    position: fixed;     /* Fixed Sidebar (stay in place on scroll and position relative to viewport) */\n    height: 100%;\n    width: 100px;     /* Set the width of the sidebar */\n    z-index: 1;      /* Stay on top of everything */\n    top: 0.0em;      /* Stay at the top */\n    background-color: grey,; \n    overflow-x: hidden;     /* Disable horizontal scroll */\n    padding-top: 20px;\n    padding-left: 5px;\n`;\n\nclass SideNav extends React.Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            activePath: props.location.pathname,\n            items: [\n                {\n                  path: '/', /* path is used as id to check which NavItem is active basically */\n                  name: 'Home',\n                  css: 'fa fa-fw fa-home',\n                  key: 1 /* Key is required, else console throws error. Does this please you Mr. Browser?! */\n                },\n                {\n                  path: '/:id/:name',\n                  name: 'Yourwishlist',\n                  css: 'fas fa-gifts',\n                  key: 2\n                },\n                {\n                    path: '/signIn',\n                    name: 'sign in',\n                    css: 'fa fa-fw fa-snowman',\n                    key: 3\n                }\n              ]\n        }\n    }\n\n    onItemClick = (path) => {\n        this.setState({ activePath: path });\n    }\n\n    render() {\n        const { items, activePath } = this.state;\n        return(\n            <StyledSideNav>\n                {\n                    items.map((item) => {\n                        return (\n                            <NavItem \n                                path={item.path}\n                                name={item.name}\n                                css={item.css}\n                                onItemClick={this.onItemClick}\n                                active={item.path === activePath}\n                                key={item.key}\n                            />\n                        );\n                    })\n                }\n            </StyledSideNav>\n        );\n    }\n}\n\nconst RouterSideNav = withRouter(SideNav);\n\nconst StyledNavItem = styled.div`\n    height: 70px;\n    width: 75px; /* width must be same size as NavBar to center */\n    text-align: center; /* Aligns <a> inside of NavIcon div */\n    margin-bottom: 0;   /* Puts space between NavItems */\n    a {\n        font-size: 2.7em;\n        color: ${(props) => props.active ? \"white\" : \"white\"};\n        opacity: 0.6;\n        :hover {\n            opacity: 1;\n            text-decoration: none; /* Gets rid of underlining of icons */\n        }  \n    }\n`;\n\nclass NavItem extends React.Component {\n    handleClick = () => {\n        const { path, onItemClick } = this.props;\n        onItemClick(path);\n    }\n\n    render() {\n        const { active } = this.props;\n        return(\n            <StyledNavItem active={active}>\n                <Link to={this.props.path} className={this.props.css} onClick={this.handleClick}>\n                    <NavIcon></NavIcon>\n                </Link>\n            </StyledNavItem>\n        );\n    }\n}\n\nconst NavIcon = styled.div`\n`;\n\nexport default class Sidebar extends React.Component {\n    render() {\n        return (\n            <RouterSideNav></RouterSideNav>\n        );\n    }\n}","/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/groupForms/GroupForms.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/pages/ErrorPage/NoMatch.js",["83","84","85","86","87","88"],"import React from 'react'\nimport { BrowserRouter as Router, Route, Switch } from \"react-router-dom\";\nimport \"./NoMatch.css\"\n\nexport default function ErrorPage() {\n    return (\n        < >\n            <div class=\"window\">\n                <div className=\"santa\">\n                <div className=\"head\">\n                    <div className=\"face\">\n                    <div className=\"redhat\">\n                        <div className=\"whitepart\"></div>\n                        <div className=\"redpart\"></div>\n                        <div className=\"hatball\"></div>\n                    </div>\n                    <div className=\"eyes\"></div>\n                    <div className=\"beard\">\n                        <div className=\"nouse\"></div>\n                        <div className=\"mouth\"></div>\n                    </div>\n                    </div>\n                    <div className=\"ears\"></div>\n                </div>\n                <div className=\"body\"></div>\n                </div>\n            </div><br></br>\n            <div className=\"message\">\n                <h1></h1>\n                <h1>404 ERROR!</h1>\n                <h2 className=\"message-2\">Only naughty boys and girls end up here!<br></br>\n                <a href=\"javascript:history.back()\">Click here to go home</a></h2>\n            </div>\n        </>\n    )\n}\n","/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/SignInComponents/LoginComp.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/com