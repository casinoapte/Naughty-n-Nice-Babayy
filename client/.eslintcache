n            items: [\n                {\n                  path: '/', /* path is used as id to check which NavItem is active basically */\n                  name: 'Home',\n                  css: 'fa fa-fw fa-home',\n                  key: 1 /* Key is required, else console throws error. Does this please you Mr. Browser?! */\n                },\n                {\n                  path: '/:id/:name',\n                  name: 'Yourwishlist',\n                  css: 'fas fa-gifts',\n                  key: 2\n                },\n                {\n                    path: '/signIn',\n                    name: 'sign in',\n                    css: 'fa fa-fw fa-snowman',\n                    key: 3\n                }\n              ]\n        }\n    }\n\n    onItemClick = (path) => {\n        this.setState({ activePath: path });\n    }\n\n    render() {\n        const { items, activePath } = this.state;\n        return(\n            <StyledSideNav>\n                {\n                    items.map((item) => {\n                        return (\n                            <NavItem \n                                path={item.path}\n                                name={item.name}\n                                css={item.css}\n                                onItemClick={this.onItemClick}\n                                active={item.path === activePath}\n                                key={item.key}\n                            />\n                        );\n                    })\n                }\n            </StyledSideNav>\n        );\n    }\n}\n\nconst RouterSideNav = withRouter(SideNav);\n\nconst StyledNavItem = styled.div`\n    height: 70px;\n    width: 75px; /* width must be same size as NavBar to center */\n    text-align: center; /* Aligns <a> inside of NavIcon div */\n    margin-bottom: 0;   /* Puts space between NavItems */\n    a {\n        font-size: 2.7em;\n        color: ${(props) => props.active ? \"white\" : \"white\"};\n        opacity: 0.6;\n        :hover {\n            opacity: 1;\n            text-decoration: none; /* Gets rid of underlining of icons */\n        }  \n    }\n`;\n\nclass NavItem extends React.Component {\n    handleClick = () => {\n        const { path, onItemClick } = this.props;\n        onItemClick(path);\n    }\n\n    render() {\n        const { active } = this.props;\n        return(\n            <StyledNavItem active={active}>\n                <Link to={this.props.path} className={this.props.css} onClick={this.handleClick}>\n                    <NavIcon></NavIcon>\n                </Link>\n            </StyledNavItem>\n        );\n    }\n}\n\nconst NavIcon = styled.div`\n`;\n\nexport default class Sidebar extends React.Component {\n    render() {\n        return (\n            <RouterSideNav></RouterSideNav>\n        );\n    }\n}","/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/groupForms/GroupForms.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/pages/HomePage/HomePage.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/pages/main-group-page/MainGroupPage.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/groupForms/findGroup/FindGroup.js",[],"/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/Yourwishlist/DisplayWishList.js",["72"],"// import WishlistModal from './WishlistModal';\nimport React, { Component } from \"react\";\nimport { Card } from \"react-bootstrap\";\nimport \"./style.css\";\n\nexport default function DisplayWishList() {\n  // Use State and Hooks Setting //\n\n  // External JS functions //\n\n  // Visual Rendering //\n\n  return (\n    <div className=\"col-4\">\n      <Card\n        style={{ width: \"20rem\" }}\n        className=\"container-fluid group-card\"\n        id=\"group-card\"\n      >\n        <Card.Body>\n          <Card.Title>Wish List</Card.Title>\n          <Card.Subtitle classNam