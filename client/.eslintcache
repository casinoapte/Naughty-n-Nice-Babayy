eteMember = (e) => {\n    e.preventDefault()\n   const userName = e.target.getAttribute(\"dataname\");\n    console.log(userName)\n\n\n\n  }\n\n\n\n\n\n  // Visual Rendering //\n\n  return (\n    <div className=\"col-4\">\n      <Card\n        style={{ width: \"20rem\" }}\n        className=\"container-fluid group-card\"\n        id=\"group-card\"\n      >\n        <Card.Body>\n          <Card.Title>Members:</Card.Title>\n          <Card.Subtitle className=\"mb-2 text-muted\">\n          </Card.Subtitle>\n          <br></br>\n          <form>\n\n            {/* Name of Member  */}\n\n            {memberList.map(member => (\n              <>\n                <p>{member.name}</p>\n                <DeleteButton name={member.name} onClick={deleteMember} />\n              </>\n            ))}\n\n            <h5>Add Person:</h5>\n            <input className=\"members-add-title\" type=\"text\" name=\"name\" onChange={handleInputChange} />\n\n            <br></br>\n\n            <button\n              className=\"add-member-button btn btn-primary\"\n              onClick={addMember}>\n              Submit\n                </button>\n          </form>\n        </Card.Body>\n      </Card>\n    </div>\n\n  )\n}\n\nexport default MembersComponent;","/Users/casinoa/Desktop/Coding/ClassWork/Naughty-n-Nice-Babayy/client/src/components/GroupDescription/DescriptionComponent.js",["74"],"// import DescriptionModal from './DescriptionModal';\nimport React, { useState, useEffect } from 'react';\nimport { useLocation } from \"react-router-dom\";\nimport API from \"../../utils/API\"\nimport { Card } from \"react-bootstrap\";\nimport \"./style.css\"\nexport function descriptionAdd() { }\n\nexport default function DescriptionComponent() {\n\n  // Use State and Hooks Setting //\n  const { pathname } = useLocation();\n\n  const [DescriptionString, setDescriptionString] = useState()\n\n  const [ShowDetails, SetDetails] = useState([])\n\n  const handleInputChange = (e) => {\n    e.preventDefault()\n    setDescriptionString({ ...DescriptionString, [e.target.name]: e.target.value })\n  }\n\n  useEffect(() => {\n    getDescription()\n  }, [])\n\n\n  // External JS functions //\n\n  // Pushing Description Array in Group Database //\n\n  const descriptionAdd = (e) => {\n    e.preventDefault();\n    const groupId = pathname.split(\"/\")[1];\n    API.addDescription(DescriptionString, groupId)\n      .catch(err => console.log(err));\n    getDescription()\n  }\n\n  const getDescription = (e) => {\n    const groupId = pathname.split(\"/\")[1];\n    API.findGroup2(groupId)\n      .then((res => {\n        let details = (res.data)\n        SetDetails(details)\n      }))\n  }\n\n\n  // Visual Rendering //\n\n  return (\n    <div className=\"col-4\">\n      <Card\n        style={{ width: \"20rem\" }}\n        className=\"container-fluid group-card\"\n        id=\"group-card\"\n      >\n        <Card.Body>\n          <Card.Title>Details:</Card.Title>\n          <Card.Subtitle className=\"mb-2 text-muted\">\n            ***\n              </Card.Subtitle>\n          <br></br>\n\n          <p className=\"details-titles\">Location: {ShowDetails.location}</p>\n          <p className=\"details-titles\">Party Time: {ShowDetails.time}</p>\n          <p className=\"details-titles\">Max Price: ${ShowDetails.price}</p>\n\n          <form>\n\n            {/* Location  */}\n\n            <h5>Location:</h5>\n            <input type=\"text\" name=\"location\" onChange={handleInputChange}></input>\n\n            {/* Time  */}\n\n            <h5>Time:</h5>\n            <input type=\"text\" name=\"time\" onChange={handleInputChange}></input>\n\n            {/* Max Gift Price  */}\n\n            <h5>Max Price ($):</h5>\n            <input type=\"number\" name=\"price\" onChange={handleInputChange}></input>\n\n            <br></br>\n\n            <button\n              className=\"add-member-button btn btn-primary\"\n              onClick={descriptionAdd}>\n              Add\n            